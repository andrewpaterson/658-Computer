@sum_even_and_odd:
    int16 i; int16 number; int16 even; int16 odd
    number = 10; i = 0; even = 0; odd = 0
bool    b
loop:
    b = i
	b %= 2
    b ?
    if= go even_
    odd += i
    go done
even_:
    even += i
done:
    i++; number ?- i
    if> go loop
    > odd
    > even
    gosub @print_pair
    return
end

@@main:
    gosub @sum_even_and_odd
    return
end

