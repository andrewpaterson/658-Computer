$start_address 0x800

@count_pointless:
    int16 start
    int16 end
    int8 value
    start <
    end <
    int8* address <
    int16 i = start
loop:
    address[i] = value
    i++
    i ?- end + 3
    if<= go loop
    i -= start
    > i
    return
end

@@main
    > 9
    > 23
    > 0
    gosub count_pointless
    int16 count <

    .count = count_pointless 9, 23, 0
	>9 >23 >0 count_pointless count<
    int32 number; int32 another
    number = 5
    another  6
    .number = number + ((3 + 6) * another) / 2
    int32 temp1 = 3; temp1 += 6; temp1 *= another; temp1 /= 2;
    number = number + temp1
end

