$start_address 0x800

struct party_address
    int8* line1
    int8* line2
    int16 country_number
end

struct person
    int8* name
    int8 age
    party_address address
	int[20] guid
end

int8*	henry_name = "Henry"

@person[3] three_people

person* henry = &person[0]
henry.age = 6
henry.name = henry_name

@run_me:
    person* person = &henry
    int8* address_line;

    address_line = person.address.line1
end

@@main:
   gosub @run_me
   return
end

